    "@testing-library/react": "^10.4.3",
    "@types/resize-observer-browser": "^0.1.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "del-cli": "^3.0.0",
